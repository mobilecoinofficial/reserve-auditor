    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "process": "^0.11.10",
    "webpack-bundle-analyzer": "^5.0.1"
  },
  "scripts": {
    "start-testnet": "webpack serve --config webpack/webpack.config.js --env env=testnet --mode development",
    "start-mainnet": "webpack serve --config webpack/webpack.config.js --env env=mainnet --mode development",
    "start-local": "webpack serve --config webpack/webpack.config.js --env env=local-main --mode development",
    "start-local-test": "webpack serve --config webpack/webpack.config.js --env env=local-test --mode development",
    "start-staging-main": "webpack serve --config webpack/webpack.config.js --env env=staging-main --mode development",
    "start-staging-test": "webpack serve --config webpack/webpack.config.js --env env=staging-test --mode development",
    "build-local": "webpack --config webpack/webpack.config.js --env env=local-main --mode production",
    "build-local-test": "webpack --config webpack/webpack.config.js --env env=local-test --mode production",
    "build-testnet": "webpack --config webpack/webpack.config.js --env env=testnet --mode production",
    "build-staging-test": "webpack --config webpack/webpack.config.js --env env=staging-test --mode production",
    "build-mainnet": "webpack --config webpack/webpack.config.js --env env=mainnet --mode production",
    "build-staging-main": "webpack --config webpack/webpack.config.js --env env=staging-main --mode production",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
